/*
6.Accept three numbers from user and find out largest number among three and also find out
whether that three numbers are equal or not.(else if ladder)
*/

#include<stdio.h>
int main()
{
int a,b,c;
printf("Enter any three number \n");
scanf("%d%d%d",&a,&b,&c);
if (a>b && a>c)
{
printf("a=%d is greater ",a);
}
else if (b>a && b>c)
{
printf("b=%d is greater ",b);
}
else if(c>a && c>b)
{
printf("c=%d is greater ",c);
}
else 
{
printf("all are eqal");
}
return 0;
}
