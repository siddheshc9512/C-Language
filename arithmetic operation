// WAP to demonstrate arithmetic operation on two integer numbers


#include <stdio.h>

int main() {
    
int a,b,c;
    
printf("Enter the value of a :");
scanf("%d", &a);
     
printf("Enter the value of b :");
scanf("%d", &b);
    
c=a+b;
printf("\n addition of two number %d",c);
 
c=a-b;
printf("\n sub of two number %d",c);
    
c=a*b;
printf("\n mul of two number %d",c);
    
c=a/b;
printf("\n dev of two number %d",c);
    
return 0;
}
