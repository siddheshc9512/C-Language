//WAP to check whether Number is positive or negative or ZERO

#include<stdio.h>
int main()
{
float num;
printf("Enter any number : ");
scanf("%f",&num);

if (num>0)
{
printf("the number is positive");
}

if (num<0)
{
printf("the number is negative");
}

if (num==0)
{
printf("the number is zero");
}
return 0;

}
