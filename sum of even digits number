/*
2. WAP to accept a number from user and find out sum of even digits from that given number.
*/

#include <stdio.h>
int main()
{
int num,total=0,i;
printf("Enter the number :");
scanf("%d",&num);
for(i=1;i<=num;i++)
{
if(i%2==0)
{
total=total+i;
printf("%d is even \n",i);
}
}
printf("\n the total is %d",total);

}
