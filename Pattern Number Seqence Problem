#include<stdio.h>
int main()
{
 int n, i,j,k;
 printf("Enter the size of n: \n ");
 scanf("%d",&n);
 for(i=1;i<=n;i++)
 {  
  for(j=1;j<=n;j++)
  {
   k=i+j-1;
   if(k>n) 
   {
     k=k-n;
   }
   printf("%d ",k );
   

  }
 printf("\n");
 }
}

/*
Input:
Enter the size of n: 4

Output:
1 2 3 4
2 3 4 1
3 4 1 2
4 1 2 3

*/
