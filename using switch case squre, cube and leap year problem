scanf("%d",&ch);

switch(ch)
{
	case 1:
		printf("\nEnter any number");
		scanf("%d",&num);
		printf("the square of num = %d\n",num*num);
		printf("cube of the number = %d\n",num*num*num);

		break;
	case 2:
		printf("\nEnter the year");
		scanf("%d",&year);
		
		if(year %4==0)
		{
			printf("\nEntered year is leap");
		}
		
		else
		{
			printf("\nEntered year is not leap");
		}

		break;
	default :
		printf("\nyou have entrered wrong choice");

}

