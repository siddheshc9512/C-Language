//WAP to calculate area of rectangle

#include<stdio.h>
int main()
{
    float length,width,Area;
    
    printf("Enter the number of length");
    scanf("%f",&length);
    
    printf("Enter the number of width");
    scanf("%f",&width);
    
    Area=length*width;
    
    printf("Area of rectangle is %f",Area);
    
    return 0;
}
