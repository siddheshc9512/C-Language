//  WAP to compare two strings using strcmp ()

#include<stdio.h>
#include<string.h>

int main()
{
char name1[20],name2[20];

printf("\n Enter the any two names \n");
scanf("%s \n %s",name1,name2);

if(strcmp(name1,name2)==0)
{
printf("\n strings are same");
}

else
{
printf("\n strins are not same");
}

return 0;
}
